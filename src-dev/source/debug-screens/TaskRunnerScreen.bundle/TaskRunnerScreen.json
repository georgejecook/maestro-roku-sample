{
  "views": [
    {
      "_type": "mv_Rectangle",
      "size": [1920, 1080],
      "color": "~colors.background.darkPurple"
    },
    {
      "id": "bgRect",
      "_type": "mv_Rectangle",
      "translation": [0, 0],
      "color": "#6a307d"
    },
    {
      "id": "startButton",
      "_type": "mv_Button",
      "text": "Start",
      "size": [100, 60],
      "autoSize": true,
      "translation": [200, 300],
      "observe": {
        "selected": "onStartButtonSelected"
      }
    },
    {
      "id": "cancelButton",
      "_type": "mv_Button",
      "size": [100, 60],
      "autoSize": true,
      "translation": [500, 300],
      "text": "Cancel",
      "observe": {
        "selected": "onCancelButtonSelected"
      }
    },
    {
      "id": "resetButton",
      "_type": "mv_Button",
      "size": [100, 60],
      "autoSize": true,
      "translation": [800, 300],
      "text": "Reset",
      "observe": {
        "selected": "onResetButtonSelected"
      }
    },
    {
      "id": "outputLabel",
      "_type": "mv_Label",
      "translation": [200, 400],
      "size": [1000, 0],
      "fontKey": "~fonts.body-06",
      "color": "~colors.dark",
      "horizAlign": "left",
      "vertlign": "top",
      "wrap": true,
      "lineSpacing": 0,
      "text": "Status"
    }
  ]
}
