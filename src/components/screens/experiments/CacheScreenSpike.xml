<?xml version='1.0' encoding='utf-8'?>
<component
    name='CacheScreenSpike'
    extends='mv_BaseScreen'
   vm='NodeTest'
  >

  <interface>
  </interface>
  <children>

    <Video id='video' />
    <Animation
        id="moveAnim"
        duration3="{(getName(value, node))}"
        duration2="{{foo}}"

        duration5='{{="hello" + "world"}}'
        duration="0.3"
        repeat="false"
        easeFunction="linear">
      <Vector2DFieldInterpolator
          id="moveAnim_interpolator"
          key="[0.0, 1.0]"
          keyValue="[ [1920, 0], [0, 6] ]" />
    </Animation>
    <Animation
        id="fadeAnim"
        duration="1"
        repeat="false"
        easeFunction="linear">
      <FloatFieldInterpolator
          id="fadeAnim_interpolator"
          key="[0.0, 1.0]"
          keyValue="[ 1.0, 0.0 ]" />
    </Animation>

  </children>
</component>